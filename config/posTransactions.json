{
  "moduleKey": "pos_transaction_management",
  "masterTable": "transactions_pos",
  "tables": {
    "transactions_pos": {
      "transaction": "POS Master",
      "multiRow": false,
      "position": "Upper left"
    },
    "transactions_posorder": {
      "transaction": "POS Order Item",
      "multiRow": true,
      "position": "Upper right"
    },
    "transactions_inventory": {
      "transaction": "Inventory Transaction",
      "multiRow": true,
      "position": "Lower left"
    },
    "transactions_income": {
      "transaction": "Income Transaction",
      "multiRow": false,
      "position": "Lower right"
    },
    "transactions_expense": {
      "transaction": "Expense Transaction",
      "multiRow": false,
      "position": "Bottom row"
    }
  },
  "calculatedFields": {
    "transactions_pos": {
      "total_amount": {
        "formula": "transactions_income.or + SUM(transactions_inventory.ap) + transactions_expense.z + SUM(transactions_posorder.inventory_price)"
      },
      "cashback": {
        "formula": "payable_amount - total_amount + total_discount"
      },
      "payable_amount": {
        "formula": "total_amount - total_discount - cashback"
      }
    }
  },
  "status": {
    "beforePost": 0,
    "afterPost": 1
  }
}
